src unfiltered \{
    {
        my $unfiltered = $squidguard{'Unfiltered'} || "";
        foreach my $el (split(',',$unfiltered)) {
            if ($el =~ /^(?:[0-9]{1,3}\.){3}[0-9]{1,3}$/) { #ip address
                $OUT.="    ip $el\n";
            } else {
                $OUT.="    user $el\n";
            }
        }
    }
\}
