    default \{
        pass all
{
    use esmith::NetworksDB;
    my $ndb = esmith::NetworksDB->open_ro();
    my $green = $ndb->green();
    my $redirect = $squidguard{'RedirectUrl'} || "http://".$green->prop('ipaddr')."/cgi-bin/squidGuard.cgi?clientaddr=%a&clientname=%n&clientident=%i&srcclass=%s&targetgroup=%t&url=%u"; 
    $OUT.="        redirect     $redirect\n";
}
    \}
